<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="@dimen/activity_main_common_topbar_height"
    android:background="@color/transparent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:id="@+id/activity_altertel_topbar_lly"
    >
    <RelativeLayout
        android:id="@+id/rly_addressmanage_topbar_leftmenu"
        android:layout_width="@dimen/zero"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_weight="1"
        >
        <ImageView
            android:id="@+id/iv_activity_altertel_topbar_leftmenu"
            android:layout_width="@dimen/activity_helpmebuy_topbar_leftmenu_width"
            android:layout_height="@dimen/activity_helpmebuy_topbar_leftmenu_height"
            android:background="@drawable/greenreturn"
            android:layout_centerInParent="true"

            android:layout_marginLeft="@dimen/activity_main_common_title_margin_size"
            />
    </RelativeLayout>
    <!--
        style="@style/main_activity_topbar_title_text_style"
        android:text="@string/main_activity_topbar_title"-->
    <!--标题-->
    <RelativeLayout
        android:layout_width="@dimen/zero"
        android:layout_height="match_parent"
        android:layout_weight="4"
        >
        <TextView
            android:id="@+id/tv_activity_altertel_topbar_title"
            android:layout_width="@dimen/activity_altertel_topbar_center_title_width"
            android:layout_height="@dimen/activity_altertel_topbar_center_title_height"
            android:background="@drawable/messagecenter"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>
    <!--标题-->
    <RelativeLayout
        android:id="@+id/rly_addressmanage_topbar_addaddress"
        android:layout_width="@dimen/zero"
        android:layout_height="match_parent"
        android:layout_weight="1"
        >

    </RelativeLayout>
</LinearLayout>